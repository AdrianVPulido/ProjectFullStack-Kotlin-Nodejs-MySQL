<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.adria.tpvandroid">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:usesCleartextTraffic="true"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ComandasActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".ProductosActivity"
                android:label="Productos"/>
        <activity
                android:name=".AddProductosActivity"
                android:label="Añadir Productos"
                android:parentActivityName=".ProductosActivity"/>
        <activity
                android:name=".AddComandasActivity"
                android:label="Añadir Comandas"
                android:parentActivityName=".ComandasActivity"/>
        <activity
                android:name=".imageactivity"
                android:label="Imagenes"
                android:parentActivityName=".ComandasActivity"/>
        <activity android:name=".ServerActivity">
        </activity>
    </application>

</manifest>